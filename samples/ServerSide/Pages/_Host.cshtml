@page "/"
@namespace ServerSide.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>BlGridServer</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/fontawesome/css/fontawesome.min.css" />
    <link href="css/fontawesome/css/all.min.css" rel="stylesheet" />
    <link href="css/fontawesome/css/brands.css" rel="stylesheet">
    <link href="css/fontawesome/css/solid.css" rel="stylesheet">
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link href="_content/DnetOverlayComponent/dnet-overlay-style.css" rel="stylesheet" />
    <link href="_content/DnetDialogComponent/dnet-dialog-style.css" rel="stylesheet" />
    <link href="_content/DnetConnectedPanel/dnet-connected-panel-style.css" rel="stylesheet" />
    <link href="_content/DnetToastComponent/dnet-toast-style.css" rel="stylesheet" />
    <link href="_content/DnetTooltip/dnet-tooltip-style.css" rel="stylesheet" />
    <link href="_content/DnetAutocompleteComponent/dnet-autocomplete-list-style.css" rel="stylesheet" />
    <link href="_content/DnetVirtualScrolling/dnet-virtualscrolling-style.css" rel="stylesheet" />
    <link href="css/site.css" rel="stylesheet" />
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="ServerPrerendered" />
    </app>

    <script src="_framework/blazor.server.js"></script>
    <script src="css/fontawesome/js/all.min.js"></script>
    <script src="_content/DnetOverlayComponent/dnet-overlay.js"></script>
    <script src="_content/DnetOverlayComponent/rxjs.min.js"></script>
    <script src="_content/DnetVirtualScrolling/virtualscrolling-interop.js"></script>
    <script src="_content/DnetAutocompleteComponent/virtualscrolling-interop.js"></script>
</body>
</html>
